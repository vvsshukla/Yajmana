<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:fillViewport="true"
    tools:context=".EditFragment">

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:indeterminate="true"
        tools:visibility="visible" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="8dp">

            <TextView
                android:id="@+id/swa_vyaktichi_mahiti"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:text="@string/swa_vyaktichi_mahiti"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="90dp"
                android:layout_height="60dp"
                android:layout_below="@+id/swa_vyaktichi_mahiti"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:contentDescription="@string/person_s_photo"
                android:padding="4dp"
                android:src="@drawable/client" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:id="@+id/LayoutFullName"
                android:layout_below="@+id/imageView"
                android:weightSum="2">

                <TextView
                    android:id="@+id/full_name_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/full_name_label"
                    android:layout_weight="1"
                    android:layout_marginTop="20dp"
                    android:padding="10dp"
                    android:textSize="14sp"
                    android:textStyle="bold" />
<!--                android:layout_marginLeft="20dp"-->
<!--                android:layout_marginStart="20dp"-->
                <EditText
                    android:id="@+id/full_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:hint="@string/full_name_label"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    android:layout_marginTop="10dp"
                    android:padding="10dp"
                    android:background="@drawable/oval"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LayoutTwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/LayoutFullName"
                android:layout_marginTop="10dp"
                android:weightSum="1">
                <!-- gaon -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/city_lable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/city_label"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/city"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/city_lable"
                        android:hint="@string/city_label"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

                <!-- taluka -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp">

                    <TextView
                        android:id="@+id/taluka_lable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/taluka_label"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/taluka"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/taluka_lable"
                        android:hint="@string/taluka_lable"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LayoutThree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/LayoutTwo"
                android:layout_marginTop="10dp"
                android:weightSum="1">

                <!-- jilha -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/district_lable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/district_label"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/district"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/district_lable"
                        android:hint="@string/district"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@drawable/oval" />

                </RelativeLayout>

                <!-- Samaj -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp">

                    <TextView
                        android:id="@+id/caste_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/caste_label"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/caste"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/caste_label"
                        android:hint="@string/caste"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LayoutFour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/LayoutThree"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:weightSum="1">
                <!-- tithi  -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/death_anniversary_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/death_anniverary_msg"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/death_anniversary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/death_anniversary_label"
                        android:hint="@string/date_of_death_label"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:digits="0123456789-"
                        android:padding="10dp"
                        android:background="@drawable/oval" />

                </RelativeLayout>
                <!-- relation  -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp">

                    <TextView
                        android:id="@+id/relation_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/relation_label"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:textStyle="bold"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/relation"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/relation_label"
                        android:hint="@string/relation_label"
                        android:importantForAutofill="no"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LayoutFive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/LayoutFour"
                android:layout_marginTop="20dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/pooja_person_names"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/pooja_person_names_label" />

                <TextView
                    android:id="@+id/pooja_person_vanshawal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="@string/add_button_tip" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LayoutSix"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/LayoutFive"
                android:layout_marginTop="20dp"
                android:orientation="vertical">

                <!-- tumche purna nav -->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/your_full_name_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/your_full_name_label"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp"
                        android:layout_marginTop="10dp" />

                    <EditText
                        android:id="@+id/your_full_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/your_full_name_label"
                        android:hint="@string/your_full_name_label"
                        android:background="@drawable/oval"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"/>
                    <!-- vadilanche purna nav -->
                    <TextView
                        android:id="@+id/your_father_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/your_full_name"
                        android:text="@string/fathers_name"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp" />

                    <EditText
                        android:id="@+id/fathers_full_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/your_father_name"
                        android:hint="@string/fathers_full_name"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"
                        android:background="@drawable/oval" />

                    <TextView
                        android:id="@+id/mothers_full_name_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/fathers_full_name"
                        android:text="@string/mothers_full_name"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp" />

                    <EditText
                        android:id="@+id/mothers_full_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/mothers_full_name_label"
                        android:hint="@string/mothers_full_name"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                    <TextView
                        android:id="@+id/grandpa_name_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/mothers_full_name"
                        android:text="@string/grandpa_name"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp"/>

                    <EditText
                        android:id="@+id/grandpa_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/grandpa_name_label"
                        android:hint="@string/grandpa_name"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"
                        android:background="@drawable/oval" />

                    <TextView
                        android:id="@+id/panjoba_name_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/grandpa_name"
                        android:text="@string/panjoba_name"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp"/>

                    <EditText
                        android:id="@+id/panjoba_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/panjoba_name_label"
                        android:hint="@string/panjoba_name"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"
                        android:background="@drawable/oval" />

                    <TextView
                        android:id="@+id/ni_panjoba_name_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/panjoba_name"
                        android:text="@string/ni_panjoba_name"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp" />

                    <EditText
                        android:id="@+id/ni_panjoba_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ni_panjoba_name_label"
                        android:hint="@string/ni_panjoba_name"
                        android:importantForAutofill="no"
                        android:inputType="textPersonName"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

                <!-- Real Uncle Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">

                    <TextView
                        android:id="@+id/real_uncle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:text="@string/real_uncle_name"
                        android:layout_marginTop="20dp"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/add_more_real_uncle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp"
                        android:textSize="24sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L1"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Cousin Uncle Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/cousin_uncle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:text="@string/cousin_uncle_name"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_cousin_uncle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L2"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Real Uncle Son Name -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/real_uncle_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:text="@string/real_uncle_son_name"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_real_uncle_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L3"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Cousin Uncle Son Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/cousin_uncle_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:text="@string/cousin_uncle_son_name"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_cousin_uncle_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L4"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Real Brother Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/real_brother"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:text="@string/real_brother_name"
                        android:textStyle="bold"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_real_brother"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L5"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Real Brother Son Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/real_brother_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:text="@string/real_brother_son_name"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_real_brother_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L6"
                    android:orientation="vertical">

                </LinearLayout>

                <!-- Real Son Name -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:padding="10dp"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:text="@string/son_name"
                        android:layout_marginTop="20dp" />
                    <TextView
                        android:id="@+id/add_more_son"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.05"
                        android:padding="10dp"
                        android:textSize="24sp"
                        android:text="@string/add_more"
                        android:gravity="center"
                        android:textColor="@color/red"
                        android:layout_marginTop="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/L7"
                    android:orientation="vertical">

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/mobile_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/mobile_no_label"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:layout_marginTop="20dp"
                        android:padding="10dp" />

                    <EditText
                        android:id="@+id/mobile_no"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/mobile_label"
                        android:hint="@string/mobile_no"
                        android:importantForAutofill="no"
                        android:inputType="phone"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                    <TextView
                        android:id="@+id/tip_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/mobile_no"
                        android:text="@string/tip"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"
                        android:padding="10dp" />

                    <EditText
                        android:id="@+id/tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tip_label"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:hint="@string/tip"
                        android:importantForAutofill="no"
                        android:padding="10dp"
                        android:background="@drawable/oval"/>

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1">

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="30dp"
                    android:layout_marginBottom="45dp">

                    <Button
                        android:id="@+id/btnUpdate"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/primary"
                        android:text="@string/save"
                        tools:targetApi="lollipop"/>

                    <Button
                        android:id="@+id/btnCancel"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
